{
  "name": "ifc.js",
  "version": "1.0.1",
  "description": "IFC viewer",
  "main": "build/IFC.js",
  "module": "build/IFC.es.js",
  "browser": "build/IFC.umd.js",
  "scripts": {
    "pretest": "npm run lint && npm run lint-spec",
    "test": "npm run test-nolint",
    "test-nolint": "karma start ./spec/karma.conf.js",
    "build": "npm run rollup && npm run uglify",
    "release": "./rollup/publish.sh",
    "lint": "eslint src",
    "lint-spec": "eslint spec/suites",
    "lintfix": "eslint src --fix; eslint spec/suites --fix;",
    "rollup": "rollup -c rollup/rollup-config.js",
    "watch": "rollup -w -c rollup/rollup-config.js",
    "uglify": "uglifyjs build/IFC.js -c -m -o build/IFC.min.js --source-map filename=build/IFC.min.js.map --comments"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agviegas/IFC.js.git"
  },
  "keywords": [
    "ifc",
    "geometry",
    "threejs",
    "bim",
    "3d"
  ],
  "author": "agviegas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agviegas/IFC.js/issues"
  },
  "homepage": "https://github.com/agviegas/IFC.js#readme",
  "dependencies": {
    "chevrotain": "^7.0.3",
    "three": "^0.123.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "prettier": "^2.0.2",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-git-version": "^0.3.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "style-loader": "^1.1.3",
    "uglify-js": "^3.8.0",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
